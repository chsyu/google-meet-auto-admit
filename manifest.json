{
   "manifest_version": 2,
   "name": "Auto Admit",
   "version": "0.3",
   "content_scripts": [
     {
       "matches": [
         "https://meet.google.com/*", "http://meet.google.com/*"
       ],
       "js": ["content.js"]
     }
   ],
   "browser_action": {
     "default_icon": "icon.png"
   }
 }